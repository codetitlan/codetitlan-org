{
  "name": "codetitlan-org",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "ubuild": "rimraf ./dist && NODE_ENV=build webpack --config ./config/webpack/webpack.config.development.js",
    "udev": "webpack-dashboard -- webpack-dev-server --config ./config/webpack/webpack.config.development.js",
    "ustart": "yarn run build && http-server ./dist/web",
    "utest": "NODE_ENV=test jest --config ./.jestconfig.json",
    "udevop": "nodemon ./dot/server.js --watch config --watch dot",
    "clean:build": "rimraf build",
    "prebuild": "yarn run clean:build",
    "build": "yarn run build:client && yarn run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.production.js --progress",
    "build:server": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.server.js --progress",
    "build:profile": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.production.js --profile --json > profile.json",
    "build:serve": "http-server build/client -p 3003 --cors -o",
    "prestart": "yarn run clean:build",
    "start": "webpack-dashboard -- node ./dot/server.js",
    "test": "cross-env BABEL_ENV=commonjs jest --no-cache --config jest.config.json",
    "test:cov": "yarn test -- --coverage",
    "test:watch": "yarn test -- --watch",
    "lint": "eslint config src/** --ext .js",
    "tunnel": "lt --port 3000"
  },
  "dependencies": {
    "@cycle/dom": "^18.1.0",
    "@cycle/http": "^14.2.0",
    "@cycle/isolate": "^3.1.0",
    "@cycle/run": "^3.2.0",
    "cycle-gun": "^0.1.6",
    "cycle-onionify": "^4.0.0",
    "hammerjs": "^2.0.8",
    "http-server": "^0.10.0",
    "immutable": "^3.8.2",
    "material-design-icons": "^3.0.1",
    "material-design-lite": "^1.3.0",
    "nodemon": "^1.11.0",
    "roboto-fontface": "^0.8.0",
    "serviceworker-webpack-plugin": "^0.2.3",
    "snabbdom": "^0.7.0",
    "snabbdom-jsx": "0.4.2",
    "union-type": "^0.4.0",
    "xstream": "11.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-builtin-classes": "0.4.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-custom-element-classes": "^0.1.0",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "buble-loader": "^0.4.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.5",
    "cssnano": "^3.10.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "21.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "flow": "^0.2.3",
    "flow-babel-webpack-plugin": "^1.1.0",
    "flow-bin": "0.55.0",
    "flow-typed": "^2.1.5",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "21.2.0",
    "lost": "^8.2.0",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "11.0.0",
    "postcss-loader": "^2.0.5",
    "prettier": "^1.7.4",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "script-loader": "^0.7.0",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dashboard": "1.0.0-3",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.6.1",
