{
  "name": "codetitlan-org",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/generators/test-generators.ts",
    "start:prod": "yarn run build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier ./src --write",
    "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@rebass/preset": "4.0.5",
    "fontfaceobserver": "2.1.0",
    "fp-ts": "2.11.5",
    "i18next": "21.3.3",
    "i18next-browser-languagedetector": "6.1.2",
    "i18next-scanner": "3.1.0",
    "react": "17.0.2",
    "react-app-polyfill": "2.0.0",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.1.2",
    "react-hook-form": "7.18.1",
    "react-i18next": "11.13.0",
    "react-redux": "7.2.6",
    "react-router-dom": "5.3.0",
    "react-scripts": "4.0.3",
    "react-test-renderer": "17.0.2",
    "rebass": "4.0.7",
    "redux-injectors": "1.3.0",
    "redux-saga": "1.1.3",
    "rxjs": "7.4.0",
    "sanitize.css": "13.0.0",
    "styled-components": "5.3.3",
    "theme-ui": "0.12.0",
    "typewriter-effect": "2.18.2",
    "web-vitals": "2.1.2"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "1.6.2",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/node-logger": "^6.3.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@types/fontfaceobserver": "2.1.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.6",
    "@types/react": "17.0.33",
    "@types/react-dom": "17.0.10",
    "@types/react-redux": "7.1.20",
    "@types/react-router-dom": "5.3.2",
    "@types/react-test-renderer": "17.0.1",
    "@types/rimraf": "3.0.2",
    "@types/shelljs": "0.8.9",
    "@types/styled-components": "5.1.15",
    "@types/testing-library__jest-dom": "5.14.1",
    "@types/webpack": "5.28.0",
    "@types/webpack-env": "1.16.3",
    "babel-loader": "8.1.0",
    "chalk": "4.1.2",
    "cross-env": "7.0.3",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.4",
    "inquirer": "8.2.0",
    "inquirer-directory": "2.2.0",
    "jest-styled-components": "7.0.5",
    "lint-staged": "11.2.6",
    "msw": "0.35.0",
    "node-plop": "0.26.3",
    "plop": "2.7.6",
    "prettier": "2.4.1",
    "react-docgen-typescript": "2.1.1",
    "rimraf": "3.0.2",
    "serve": "12.0.1",
    "shelljs": "0.8.4",
    "storybook-react-i18next": "1.0.12",
    "stylelint": "14.0.1",
    "stylelint-config-recommended": "6.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-node": "10.4.0",
    "typescript": "4.4.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "yarn": ">=1.22.0",
    "node": ">=14.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run checkTs && lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/stories/**/*.{js,jsx,ts,tsx}",
      "!src/mocks/**/*.{js,jsx,ts,tsx}",
      "!src/internals/**/*",
      "!src/utils/**/*",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/utils/loadable.tsx",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/**/*/*.test.ts",
      "!src/**/*/*.stories.tsx",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "msw": {
    "workerDirectory": "public"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
